  "scripts": {
    "build:dev": "webpack-dev-server --open --config webpack.dev.js",
    "build:prod": "webpack-dev-server --open --config webpack.prod.js",
    "build:dist": "webpack --config webpack.prod.js",
    "build:artifact": "node ci/zip.js",
    "start": "yarn build:dev",
    "prod": "yarn install && yarn build:prod",
    "dist": "yarn install && yarn build:dist",
    "artifact": "yarn install && yarn build:artifact",
    "test": "jest --watch",
    "snapshots": "jest --updateSnapshot",
    "coverage": "jest --coverage"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/tests/test-preprocessor.js"
    },
    "testMatch": [
      "**/?(*.)+(spec|test).ts?(x)"
    ],
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "<rootDir>/tests/__mocks__/styleMock.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/tests/__mocks__/fileMock.js"
    },
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "setupFiles": [
      "<rootDir>/tests/test-shim.js",
      "<rootDir>/tests/test-setup.js"
    ]
  },
  "dependencies": {
    "axios": "0.18.0",
    "lodash": "4.17.11",
