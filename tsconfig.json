{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "module": "commonjs",
    "noImplicitAny": true,
    "outDir": "./build/",
    "preserveConstEnums": true,
    "removeComments": true,
    "sourceMap": true,
    "target": "es5",
    "types": ["node"]
  },
  "include": [
    "./src/**/*"
  ],
  "exclude": [
    "node_modules",
    "vendor"
  ]
}
